---
import Footer from "../components/Footer.astro";
import Terminal from "../components/Terminal.astro";
import Form from "../components/Form";
import Layout from "../layouts/Layout.astro";
import Analytics from "@vercel/analytics/astro";

import Logo from "@/components/Brand";
import { Image } from "astro:assets";

import TerminalImage from "../assets/legal-hero.svg";

import PostHogLayout from "../layouts/PostHogLayout.astro";

import TermsHtml from "../assets/terms.html?raw";

---

<PostHogLayout>
  <Layout>
    <!-- HERO -->
    <div class="relative">
      <div
        class="mx-auto max-w-sm md:max-w-4xl xl:max-w-7xl relative z-10 px-4 md:px-0"
      >
        <header class="flex justify-between items-center py-4 md:py-8">
          <a href="/">
          <Logo /></a>

          <nav class="flex items-center gap-4">
            <!-- <a href="#">Careers</a>
            <a href="#">Documentation</a> -->
            <a
              href="#get-in-touch"
              class="bg-brand-green text-black font-semibold jetbrains-mono text-sm uppercase px-4 py-2 rounded hover:bg-brand-green-hover transition-colors duration-200"
            >
              Get In Touch
            </a>
          </nav>
        </header>

        <div
          class="flex flex-col-reverse md:flex-row items-center justify-center mx-auto max-w-sm md:max-w-4xl xl:max-w-7xl relative z-10"
        >
          <div class="flex-1 mt-4">
            <svg
              width="14"
              height="32"
              class="mb-6"
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
